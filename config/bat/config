# Register syntaxes
--map-syntax "*.h:C++"
--map-syntax "*.ino:C++"
--map-syntax "ignore:Git Ignore"
--map-syntax ".ignore:Git Ignore"
--map-syntax ".fdignore:Git Ignore"
--map-syntax ".gitconfig.local:Git Config"
--map-syntax ".zshrc.local:Bourne Again Shell (bash)"
--map-syntax ".zshenv.local:Bourne Again Shell (bash)"
--map-syntax ".local:Bourne Again Shell (bash)"
--map-syntax "*.{re,rei}:JavaScript (Babel)"
--map-syntax "yarn.lock:YAML"
